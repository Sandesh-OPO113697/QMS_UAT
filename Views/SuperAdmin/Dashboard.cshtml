@using System.Data
@model System.Data.DataTable
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor

@{
    var mySessionValue = HttpContextAccessor.HttpContext.Session.GetString("UserType");
}

@if (mySessionValue == "SuperAdmin")

{

<div class="container">
    <h5 class="card-title">Welcome SuperAdmin</h5>
    <table id="SuperAdminTable" class="table table-bordered table-hover mb-0">
        <thead>
            <tr>
                <th>AccountID</th>
                <th>AccountName</th>
                <th>Authantication_Type</th>
                <th>Isactive</th>
                <th>Create_date</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (DataRow row in Model.Rows)
            {
                <tr>
                    <td>@row["AccountID"]</td>
                    <td>@row["AccountName"]</td>
                    <td>@row["Authantication_Type"]</td>
                    <td>@(row["Isactive"].ToString() == "1" ? "Yes" : "No")</td>
                    <td>@row["Create_date"]</td>
                    
                </tr>
            }
        </tbody>
    </table>
</div>
}

        
@if (mySessionValue == "Admin")

{
    
